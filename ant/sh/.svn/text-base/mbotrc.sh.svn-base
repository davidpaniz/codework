# ~/.bashrc:   . bin/mbotrc.sh

LISTF=/task/bot/list_fed.txt
LISTE=/task/bot/list_est.txt
LISTC=/task/bot/list_cnc.txt
LISTM=/task/bot/list_mun.txt

alias mbot-sf="/task/bot/mbot.sh -mode sql -list $LISTF"
alias mbot-se="/task/bot/mbot.sh -mode sql -list $LISTE"
alias mbot-sc="/task/bot/mbot.sh -mode sql -list $LISTC"
alias mbot-sm="/task/bot/mbot.sh -mode sql -list $LISTM"

alias mbot-af="/task/bot/mbot.sh -mode ant -list $LISTF"
alias mbot-ae="/task/bot/mbot.sh -mode ant -list $LISTE"
alias mbot-ac="/task/bot/mbot.sh -mode ant -list $LISTC"
alias mbot-am="/task/bot/mbot.sh -mode ant -list $LISTM"

export LISTF LISTE LISTC LISTM

